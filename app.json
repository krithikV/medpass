{
  "expo": {
    "name": "Medpass",
    "slug": "medpass",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/medi-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/medi-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs access to location to provide location-based services."
      },
      "bundleIdentifier": "com.ksm.medpass"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/medi-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "package": "com.ksm.medpass"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "59b221bf-75b9-416d-8ec1-86d961bb773a"
      }
    },
    "owner": "krithik_2005",
    "plugins": [
      "react-native-edge-to-edge"
    ]
  }
}
